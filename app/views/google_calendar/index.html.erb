<h1>GoogleCalendar#index</h1>
<p>Find me in app/views/google_calendar/index.html.erb</p>
<p><% @calendars['items'].each do |calendar| %>
      <%= calendar['summary'] %><br>
   <% end %>
</p>
<p>
<% @event_list['items'].each do |event| %>
<strong><%= event['summary'] %></strong> |
<% if event['start']['date'].nil? %>
  <%= event['start']['dateTime'].to_datetime.strftime('%B %e, %Y at %I:%M %P') %><br>
<% else %>
  <%= event['start']['date'].to_datetime.strftime('%B %e, %Y') %><br>
<% end %>
<% end %>
</p>